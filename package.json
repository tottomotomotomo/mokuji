{
  "name": "mokuji",
  "displayName": "Mokuji",
  "publisher": "tottomotomotomo",
  "description": "CSS Table of Contents based on comments",
  "version": "0.0.1",
  "license": "MIT",
  "icon": "icon.png",
  "keywords": [
    "css",
    "scss",
    "less",
    "table of contents",
    "toc",
    "navigation",
    "outline",
    "comments",
    "mokuji"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tottomotomotomo/mokuji.git"
  },
  "bugs": {
    "url": "https://github.com/tottomotomotomo/mokuji/issues"
  },
  "homepage": "https://github.com/tottomotomotomo/mokuji#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mokuji-explorer",
          "title": "Mokuji",
          "icon": "$(list-unordered)"
        }
      ]
    },
    "views": {
      "mokuji-explorer": [
        {
          "id": "mokujiView",
          "name": "Table of Contents"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x",
    "typescript": "^5.0.0"
  }
}